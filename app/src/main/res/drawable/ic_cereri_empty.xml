<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/background">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/white" />
            <stroke
                android:width="1dp"
                android:color="@color/colorAccent" />
            <size
                android:width="96dp"
                android:height="96dp" />
        </shape>
    </item>
    <item
        android:id="@+id/image"
        android:bottom="@dimen/space_small"
        android:drawable="@drawable/ic_assignment_black_48dp"
        android:left="@dimen/space_small"
        android:right="@dimen/space_small"
        android:top="@dimen/space_small">
    </item>
</layer-list>